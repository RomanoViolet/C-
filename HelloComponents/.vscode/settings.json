{
    "editor.renderWhitespace": "all",
    "editor.insertSpaces": true,
    "editor.parameterHints.enabled": true,
    "editor.codeLens": true,
    "editor.formatOnSaveMode": "file",
    "editor.formatOnType": true,
    "editor.formatOnPaste": true,
    "editor.find.loop": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "editor.formatOnSave": true,
    "C_Cpp.enhancedColorization": "Enabled",
    "C_Cpp.intelliSenseEngine": "Default",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "C_Cpp.suggestSnippets": true,
    "C_Cpp.loggingLevel": "Debug",
    "C_Cpp.exclusionPolicy": "checkFilesAndFolders",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/CoreFunctions/**"
    ],
    "C_Cpp.default.cppStandard": "c++14",
    "C_Cpp.inactiveRegionBackgroundColor": "gray",
    "C_Cpp.clang_format_path": "/usr/bin/clang-format-14",
    "C_Cpp.codeAnalysis.clangTidy.config": "",
    "C_Cpp.clang_format_sortIncludes": true,
    "C_Cpp.autocompleteAddParentheses": true,
    "C_Cpp.codeAnalysis.clangTidy.checks.enabled": [
        "*"
    ],
    "C_Cpp.codeAnalysis.clangTidy.enabled": true,
    "C_Cpp.codeAnalysis.clangTidy.path": "/usr/bin/clang-tidy-14",
    "C_Cpp.codeAnalysis.clangTidy.args": [
        "-std=c++14"
    ],
    "C_Cpp.codeAnalysis.runAutomatically": true,
    "C_Cpp.default.intelliSenseMode": "clang-x64",
    "C_Cpp.dimInactiveRegions": true,
    "C_Cpp.formatting": "clangFormat",
    "C_Cpp.autoAddFileAssociations": true,
    "C_Cpp.clang_format_fallbackStyle": "none",
    "cmake.configureOnOpen": false,
    "files.associations": {
        "algorithm": "cpp",
        "string": "cpp",
        "iostream": "cpp",
        "sstream": "cpp",
        "iosfwd": "cpp",
        "limits": "cpp",
        "cmath": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "*.tcc": "cpp",
        "cstdint": "cpp",
        "deque": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "map": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "optional": "cpp",
        "set": "cpp",
        "string_view": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "fstream": "cpp",
        "initializer_list": "cpp",
        "iomanip": "cpp",
        "istream": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "cinttypes": "cpp",
        "typeinfo": "cpp",
        "numeric": "cpp",
        "random": "cpp",
        "any": "cpp",
        "bit": "cpp",
        "cerrno": "cpp",
        "compare": "cpp",
        "queue": "cpp",
        "variant": "cpp"
    },
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "C_Cpp.default.compileCommands": "${workspaceFolder}/build/compile_commands.json",
    "terminal.integrated.cwd": ".",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true
}
